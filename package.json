{
  "publisher": "Wuzh994",
  "name": "idiom-solitaire",
  "displayName": "成语接龙",
  "version": "0.1.3",
  "private": true,
  "packageManager": "pnpm@7.5.1",
  "description": "老少皆宜的民间文化娱乐活动",
  "author": "Wuzh994",
  "license": "MIT",
  "homepage": "https://github.com/Wuzh994/idiom-solitaire#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Wuzh994/idiom-solitaire"
  },
  "bugs": {
    "url": "https://github.com/Wuzh994/idiom-solitaire/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "resources/logo.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.69.0"
  },
  "activationEvents": [
    "onCommand:idiom-solitaire.startUp"
  ],
  "contributes": {
    "commands": [
      {
        "command": "idiom-solitaire.startUp",
        "title": "成语接龙（现在开始）"
      }
    ],
    "keybindings": {
      "command": "idiom-solitaire.startUp",
      "key": "Ctrl Ctrl"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@antfu/ni": "^0.16.3",
    "@babel/types": "^7.18.8",
    "@types/node": "^18.0.3",
    "@types/vscode": "^1.69.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.19.0",
    "esno": "^0.16.3",
    "pnpm": "^7.5.1",
    "rimraf": "^3.0.2",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4",
    "vite": "^2.9.14",
    "vitest": "^0.18.0",
    "vsce": "^2.9.2"
  }
}
